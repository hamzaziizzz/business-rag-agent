RAG_LOG_LEVEL=INFO

# Vector store
RAG_VECTORSTORE=milvus
RAG_MAX_CHUNKS=4
RAG_MIN_SCORE=0.35

# Auth
RAG_API_KEYS=YOUR_API_KEY_HERE
RAG_ALLOW_ANONYMOUS=false

# Upload limits (10 MB)
RAG_FILE_MAX_BYTES=10485760

# Answering (LLM on)
RAG_ANSWERER=llm
RAG_LLM_PROVIDER=ollama
RAG_LLM_CONTEXT_MAX_CHARS=12000
RAG_LLM_FALLBACK=refuse
RAG_LLM_GATE_ENABLED=true
RAG_LLM_GATE_MAX_TOKENS=128
RAG_LLM_GATE_TIMEOUT=20

# Query rewriting
RAG_QUERY_REWRITER=on
RAG_QUERY_REWRITER_PROVIDER=ollama
RAG_QUERY_REWRITER_MODEL=
RAG_QUERY_REWRITER_TIMEOUT=10
RAG_QUERY_REWRITER_MAX_TOKENS=64

# Router
RAG_ROUTER_MODE=rules
RAG_ROUTER_PROVIDER=ollama
RAG_ROUTER_MODEL=
RAG_ROUTER_TIMEOUT=10
RAG_ROUTER_MAX_TOKENS=64

# Chat
RAG_CHAT_HISTORY_TURNS=6

# Ollama
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.1
OLLAMA_TEMPERATURE=0.1
OLLAMA_MAX_TOKENS=512
OLLAMA_TIMEOUT=60

# Embeddings
EMBEDDING_PROVIDER=openai
EMBEDDING_DIMENSION=1536
OPENAI_API_KEY=YOUR_OPENAI_KEY_HERE
OPENAI_EMBEDDING_MODEL=text-embedding-3-small
GEMINI_API_KEY=
GEMINI_EMBEDDING_MODEL=

# Chunking
RAG_CHUNK_SIZE=1000
RAG_CHUNK_OVERLAP=100
RAG_CHUNK_TOKENS=512
RAG_CHUNK_TOKEN_OVERLAP=64
RAG_TOKENIZER_ENCODING=cl100k_base

# Milvus
MILVUS_URI=http://localhost:19530
MILVUS_TOKEN=
MILVUS_COLLECTION=business_documents
MILVUS_CONSISTENCY=Strong
MILVUS_INDEX_TYPE=HNSW
MILVUS_METRIC_TYPE=COSINE
MILVUS_HNSW_M=30
MILVUS_HNSW_EF_CONSTRUCTION=360
MILVUS_HNSW_EF=64
MILVUS_NLIST=1024
MILVUS_NPROBE=10
